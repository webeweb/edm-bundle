{
    "name": "webeweb/edm-bundle",
    "type": "symfony-bundle",
    "description": "An Electronic Document Management for Symfony 2",
    "keywords": [
        "bundle",
        "document",
        "EDM",
        "electronic",
        "management",
        "symfony",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/edm-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "ext-zip": "*",
        "doctrine/orm": "^2.5",
        "symfony/asset": "^3.4|^4.0",
        "symfony/form": "^3.4|^4.0",
        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/translation": "^3.4|^4.0",
        "twig/twig": "^1.0||^2.0",
        "webeweb/bootstrap-bundle": "^3.4",
        "webeweb/core-bundle": "^2.20",
        "webeweb/core-library": "^6.9",
        "webeweb/jquery-datatables-bundle": "^3.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0",
        "doctrine/doctrine-bundle": "^1.7",
        "monolog/monolog": "^1.18",
        "sensio/framework-extra-bundle": "^3.4|^4.0",
        "symfony/browser-kit": "^3.4|^4.0",
        "symfony/css-selector": "^3.4|^4.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/templating": "^3.4|^4.0",
        "symfony/security-bundle": "^3.4|^4.0",
        "symfony/twig-bundle": "^3.4|^4.0",
        "symfony/validator": "^3.4|^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\EDMBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\EDMBundle\\Tests\\": "Tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/edm-bundle/issues"
    }
}
