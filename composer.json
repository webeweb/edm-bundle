{
    "name": "webeweb/edm-bundle",
    "type": "symfony-bundle",
    "description": "An Electronic Document Management for Symfony 4 and more",
    "keywords": [
        "bundle",
        "document",
        "EDM",
        "electronic",
        "management",
        "symfony",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/edm-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "ext-zip": "*",
        "doctrine/orm": "^2.5.13",
        "sensio/framework-extra-bundle": "^4.4|^5.0",
        "symfony/asset": "^4.4|^5.0",
        "symfony/form": "^4.4|^5.0",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/translation": "^4.4|^5.0",
        "symfony/validator": "^4.4|^5.0",
        "twig/twig": "^2.10|^3.0",
        "webeweb/bootstrap-bundle": "^4.0",
        "webeweb/core-bundle": "^3.22",
        "webeweb/core-library": "^8.34",
        "webeweb/jquery-datatables-bundle": "^3.34"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0",
        "doctrine/doctrine-bundle": "^1.8.1|^2.0",
        "masterminds/html5": "^2.6",
        "symfony/browser-kit": "^4.4|^5.0",
        "symfony/css-selector": "^4.4|^5.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/security-bundle": "^4.4|^5.0",
        "symfony/twig-bundle": "^4.4|^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\EDMBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\EDMBundle\\Tests\\": "Tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/edm-bundle/issues"
    }
}
