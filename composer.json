{
    "name": "webeweb/edm-bundle",
    "type": "symfony-bundle",
    "description": "An Electronic Document Management for Symfony 2",
    "keywords": [
        "bundle",
        "document",
        "EDM",
        "electronic",
        "management",
        "symfony",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/edm-bundle",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb/"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "ext-zip": "*",
        "doctrine/orm": "^2.2",
        "symfony/asset": "^2.8|^3.0|^4.0",
        "symfony/form": "^2.8|^3.0|^4.0",
        "symfony/framework-bundle": "^2.8|^3.0|^4.0",
        "symfony/translation": "^2.8|^3.0|^4.0",
        "twig/twig": "^1.0||^2.0",
        "webeweb/bootstrap-bundle": "^1.7",
        "webeweb/core-library": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^7.0",
        "doctrine/doctrine-bundle": "^1.0",
        "sensio/framework-extra-bundle": "^2.8|^3.0|^4.0",
        "symfony/browser-kit": "^2.8|^3.0|^4.0",
        "symfony/css-selector": "^2.8|^3.0|^4.0",
        "symfony/monolog-bundle": "^2.8|^3.0|^4.0",
        "symfony/templating": "^2.8|^3.0|^4.0",
        "symfony/security-bundle": "^2.8|^3.0|^4.0",
        "symfony/twig-bundle": "^2.8|^3.0|^4.0",
        "symfony/validator": "^2.8|^3.0|^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\EDMBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\EDMBundle\\Tests\\": "Tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/edm-bundle/issues"
    }
}
